{
    "name": "custom-site",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^13.5.0",
        "bootstrap": "^5.1.3",
        "react": "^18.0.0",
        "react-bootstrap": "^2.2.2",
        "react-dom": "^18.0.0",
        "react-icons": "^4.3.1",
        "react-scripts": "5.0.0",
        "sass": "^1.49.11",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "npm run watch:css & react-scripts start",
        "build": "npm run format & npm run build:css & react-scripts build",
        "eject": "react-scripts eject",
        "format": "npx prettier --write .",
        "build:css": "postcss src/tailwind/index.css -o src/assets/main.css",
        "watch:css": "postcss src/tailwind/index.css -o src/assets/main.css"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "autoprefixer": "^10.4.5",
        "postcss": "^8.4.12",
        "postcss-cli": "^9.1.0",
        "prettier": "2.6.2",
        "tailwindcss": "^3.0.24"
    }
}
